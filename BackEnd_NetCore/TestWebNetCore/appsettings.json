{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtConfig": {
    "Key": "SecretKeyToGenerateJWTToken1234567890", // Key cần giữ bí mật, thực tế ko lưu ở đây
    "Issuer": "https://localhost:7031/", // Nhà cung cấp token
    "Audience": "https://localhost:7031/", // Đối tượng sử dụng token
    "DurationInMinutes": 60 // Thời gian sống của token
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=Bakery;Username=postgres;Password=123456"
  }
}
